	<message key="plugins.importexport.crossref.cliUsage">Brug: 
{$scriptName} {$pluginName} [xmlFileName] [journal_path] articles [articleId1] [articleId2] ...
{$scriptName} {$pluginName} [xmlFileName] [journal_path] nummer [issueId]</message>
	<message key="plugins.importexport.crossref.settings.depositorIntro">Indsæt navn og e-mail på den person, der er ansvarlig for CrossRef DOI-deponering.</message>
	<message key="plugins.importexport.crossref.settings.form.depositorName">Deponentens navn</message>
	<message key="plugins.importexport.crossref.settings.form.depositorEmail">Deponentens e-mail</message>
